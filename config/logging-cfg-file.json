{
  "def": {
    "type": "console",
    "options": {
      "out": "stdout"
    },
    "format": "plain",
    "format_options": {
      "delim": " ",
      "min_level_len": 5,
      "min_msg_len": 40,
      "enable_color": true,
      "enable_caller": true
    },
    "levels": [
      {"id": 5, "name": "debug"},
      {"id": 4, "name": "info", "color": 36},
      {"id": 3, "name": "warn"},
      {"id": 2, "name": "error", "color": 31},
      {"id": 1, "name": "fatal", "stacktrace": true},
      {"id": 0, "name": "panic", "stacktrace": true}
    ]
  }
}